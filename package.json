{
  "name": "mangadex-api",
  "version": "4.4.0",
  "description": "mangadex api wrapper with known apis",
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "lint": "eslint ./src --ignore-pattern *.test.*",
    "lint:fix": "eslint ./src --ignore-pattern *.test.* --fix",
    "test": "jest --config jest.json",
    "pretest": "npm run build",
    "pb": "npm run build && np",
    "build:docs": "typedoc",
    "deploy": "npm run build && npm run build:docs && gh-pages -t -d docs -b gh-pages"
  },
  "engines": {
    "node": ">=12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "keywords": [
    "mangadex",
    "manga"
  ],
  "files": [
    "dist",
    "types",
    "langcodes.json"
  ],
  "type": "commonjs",
  "repository": "github:ejnshtein/mangadex-api",
  "homepage": "https://ejnshtein.github.io/mangadex-api/",
  "author": "ejnshtein",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "smol-request": "^2.1.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.22",
    "@types/eslint": "^7.2.6",
    "@types/jest": "^26.0.16",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-plugin-add-import-extension": "^1.4.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "joi": "^17.3.0",
    "np": "^7.0.0",
    "prettier": "^2.2.1",
    "standard": "^16.0.3",
    "ts-jest": "^26.4.4",
    "typedoc": "^0.19.2",
    "typedoc-plugin-nojekyll": "^1.0.1",
    "typedoc-plugin-sourcefile-url": "^1.0.6",
    "typescript": "^4.1.2"
  }
}
