{
  "name": "mangadex-api",
  "version": "4.0.0",
  "description": "mangadex api wrapper with known apis",
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint ./src --ignore-pattern *.test.*",
    "lint:fix": "eslint ./src --ignore-pattern *.test.* --fix",
    "test": "jest --config jest.json",
    "pb": "np",
    "build:docs": "typedoc",
    "deploy": "npm run build && npm run build:docs && gh-pages -d docs -b gh-pages"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "keywords": [
    "mangadex",
    "manga"
  ],
  "files": [
    "dist",
    "types"
  ],
  "type": "commonjs",
  "repository": "github:ejnshtein/mangadex-api",
  "author": "ejnshtein",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "smol-request": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-typescript": "^7.10.4",
    "@types/eslint": "^7.2.0",
    "@types/node": "^14.6.4",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-plugin-add-import-extension": "^1.4.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "np": "^7.0.0",
    "prettier": "^2.1.2",
    "standard": "^14.3.4",
    "typedoc": "^0.19.2",
    "typedoc-plugin-nojekyll": "^1.0.1",
    "typedoc-plugin-sourcefile-url": "^1.0.6",
    "typescript": "^4.0.3"
  }
}
